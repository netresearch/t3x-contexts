{
    "name": "netresearch/contexts",
    "type": "typo3-cms-extension",
    "description": "Contexts for TYPO3 - can be used to modify page behaviour in the frontend based on several conditions",
    "homepage": "https://github.com/netresearch/t3x-contexts/",
    "license": [
        "AGPL-3.0-or-later"
    ],
    "keywords": [
        "TYPO3",
        "extension",
        "contexts"
    ],
    "support": {
        "issues": "https://github.com/netresearch/t3x-contexts/issues"
    },
    "authors": [
        {
            "name": "Andre HÃ¤hnel, Christian Opitz, Christian Weiske, Marian Pollzien, Rico Sonntag, Benni Mack",
            "email": "typo3.org@netresearch.de",
            "role": "Developer",
            "homepage": "https://www.netresearch.de"
        }
    ],
    "config": {
        "platform": {
            "php": "7.4"
        },
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true,
            "phpro/grumphp": true
        }
    },
    "require": {
        "ext-pdo": "*",
        "typo3/cms-core": "^11.5",
        "typo3/cms-frontend": "^11.5",
        "typo3/cms-backend": "^11.5",
        "typo3/cms-extbase": "^11.5",
        "typo3/cms-extensionmanager": "^11.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "squizlabs/php_codesniffer": "^3.7",
        "phpro/grumphp": "^1.13.0",
        "ssch/typo3-rector": "^2.0.3 || ^3.0.0"
    },
    "autoload": {
        "psr-4": {
            "Netresearch\\Contexts\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netresearch\\Contexts\\Tests\\": "Tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "typo3/cms": {
            "extension-key": "contexts"
        }
    }
}
