{
    "name": "netresearch/contexts",
    "type": "typo3-cms-extension",
    "description": "Contexts for TYPO3 - can be used to modify page behaviour in the frontend based on several conditions",
    "homepage": "https://github.com/netresearch/t3x-contexts/",
    "license": [
        "AGPL-3.0-or-later"
    ],
    "keywords": [
        "TYPO3",
        "extension",
        "contexts"
    ],
    "support": {
        "issues": "https://github.com/netresearch/t3x-contexts/issues"
    },
    "authors": [
        {
            "name": "Andre HÃ¤hnel, Christian Opitz, Christian Weiske, Marian Pollzien, Rico Sonntag, Benni Mack",
            "email": "typo3.org@netresearch.de",
            "role": "Developer",
            "homepage": "https://www.netresearch.de"
        }
    ],
    "config": {
        "platform": {
            "php": "8.2"
        },
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true,
            "phpro/grumphp": true,
            "a9f/fractor-extension-installer": true
        },
        "audit": {
            "abandoned": "ignore",
            "block-insecure": false
        },
        "sort-packages": true
    },
    "require": {
        "php": "^8.2",
        "ext-pdo": "*",
        "typo3/cms-backend": "^12.4 || ^13.4",
        "typo3/cms-core": "^12.4 || ^13.4",
        "typo3/cms-extbase": "^12.4 || ^13.4",
        "typo3/cms-extensionmanager": "^12.4 || ^13.4",
        "typo3/cms-frontend": "^12.4 || ^13.4"
    },
    "require-dev": {
        "a9f/typo3-fractor": "^0.5",
        "friendsofphp/php-cs-fixer": "^3.64",
        "phpro/grumphp": "^2.0",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpunit/phpunit": "^10.5 || ^11.0",
        "rector/rector": "^2.0",
        "ssch/typo3-rector": "^3.0",
        "typo3/testing-framework": "^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Netresearch\\Contexts\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netresearch\\Contexts\\Tests\\": "Tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        },
        "typo3/cms": {
            "extension-key": "contexts"
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.org"
        }
    }
}
